{
  "name": "music-jinn-api",
  "version": 2,
  "env": {
    "MONGO_DB": "@mongodb-uri"
  },
  "builds": [{ "src": "dist/app.js", "use": "@now/node-server" }],
  "routes": [
    {
      "src": "/",
      "dest": "/app.js",
      "methods": ["GET"]
    },
    {
      "src": "/songs",
      "dest": "/routes/songs.js",
      "methods": ["GET"]
    },
    {
      "src": "/jinn",
      "dest": "/routes/jinn.js",
      "methods": ["GET"]
    }
  ]
}
